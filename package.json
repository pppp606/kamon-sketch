{
  "name": "kamon-sketch",
  "version": "1.0.0",
  "description": "Node.js + TypeScript project for kamon sketch",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && node fix-imports.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "dev": "tsc --watch",
    "dev:build": "tsc && node fix-imports.js",
    "start": "node dist/cli.js",
    "serve": "live-server --port=8000 --no-browser",
    "dev:server": "concurrently \"npm run dev\" \"npm run serve\"",
    "ci": "npm run typecheck && npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "nodejs",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "canvas": "^3.2.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "live-server": "^1.2.2",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "p5": "^2.0.5",
    "tslib": "^2.8.1"
  }
}
